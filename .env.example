# ===== PostgreSQL (wiki-db) =====
POSTGRES_DB=olala
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_PORT=5432

# ===== Backend =====
BACKEND_PORT=8080
CORS_ORIGINS=http://localhost:5175,http://127.0.0.1:5175

# DB connection consumed by backend/settings
DB_HOST=wiki-db
DB_PORT=5432
DB_NAME=olala
DB_USER=postgres
DB_PASSWORD=postgres
DATABASE_URL=

# ===== LLM / Embedding =====
OLLAMA_URL=http://ollama:11434
OLLAMA_TIMEOUT=120
EMBED_MODEL=nomic-embed-text
EMBED_DIM=768
WIKI_EMBEDDINGS_READY=false

# Stage model settings (default: ollama)
SLM1_BASE_URL=http://ollama:11434
SLM1_API_KEY=ollama
SLM1_MODEL=gemma3:4b
SLM2_BASE_URL=http://ollama:11434
SLM2_API_KEY=ollama
SLM2_MODEL=gemma3:4b
JUDGE_BASE_URL=http://ollama:11434
JUDGE_API_KEY=ollama
JUDGE_MODEL=gemma3:4b

# Optional external APIs
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=

# ===== Runtime =====
LOG_LEVEL=INFO
